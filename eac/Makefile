#
#Change the name of the compiler you are using, (eg. g++-mp-5)
#
CXX       = g++
#Use: 
#make -k [DEBUG=yes] [VERBOSE=yes] [WITHOUT_PLOT_STAT=yes] [WITH_OPEN_BLAS=yes]
#
# For Windows use 
# eg. mingw32-make -k -f Makefile DEBUG=yes VERBOSE=yes
# eg. mingw32-make -k -f Makefile DEBUG=no VERBOSE=no WITHOUT_PLOT_STAT=no
# eg. mingw32-make -k -f Makefile DEBUG=no VERBOSE=no WITHOUT_PLOT_STAT=yes
#
# For Linux use
# eg. make -k -f Makefile DEBUG=yes VERBOSE=yes
# eg. make -k -f Makefile DEBUG=no VERBOSE=no WITHOUT_PLOT_STAT=no
# eg. make -k -f Makefile DEBUG=no VERBOSE=no WITHOUT_PLOT_STAT=yes
# eg  make -k -f Makefile DEBUG=no VERBOSE=no WITH_OPEN_BLAS=yes WITHOUT_PLOT_STAT=yes
#

LEAC      = ../include
OPENBLAS  = ../openblas
SSEKERNEL = ../sse_kernel

ifndef TOPDIR
TOPDIR = .
endif

ifeq ($(VERBOSE),yes)
  VERBOSE_FLAG = -D __VERBOSE_YES
else
  VERBOSE_FLAG =
endif

ifeq ($(DEBUG),yes)
  OPTIMIZE_FLAG = -O0 -g3
else
  OPTIMIZE_FLAG = -O3 -D NDEBUG
endif

ifeq ($(WITHOUT_PLOT_STAT),yes)
  CONDITIONAL_FLAG = -D __WITHOUT_PLOT_STAT
else
  CONDITIONAL_FLAG = 
endif

#
INCLUDE = -I$(TOPDIR) -I$(LEAC) -I ../include_inout

ifeq ($(WITH_OPEN_BLAS),yes)
#It is recommended to compile with the '-static' option,
#if you don't want to comment out the following line:
OPEN_BLAS_FLAG = -D __WITH_OPEN_BLAS -static
#
LIBRARIES +=  $(OPENBLAS)/liblapacke.a $(OPENBLAS)/liblapack.a $(OPENBLAS)/libcblas.a $(OPENBLAS)/libopenblas.a  $(SSEKERNEL)/libssekernel.a -lgfortran -lquadmath
INCLUDE += -I$(OPENBLAS) -I$(SSEKERNEL)
else	
OPEN_BLAS_FLAG = -D __WITHOUT_OPEN_BLAS
endif


ifeq ($(DEBUG),yes)
  PFLAGS = -O0 -Wall -g3
else
  PFLAGS = -O2 -Wall -D NDEBUG
endif


ifeq ($(OS),Windows_NT)
    EXEEXT = .exe
    COPY   = copy
    SEPARATOR = &&
    DESBIN = ..\\bin\\
else 
    LIBRARIES += -lstdc++
    COPY   = cp
   SEPARATOR = ;
    DESBIN = ../bin/
endif


.SUFFIXES: .cpp .cxx .cc .C .c
CFLAGS    = -std=c++11 -Wall $(INCLUDE) $(OPTIMIZE_FLAG) $(CONDITIONAL_FLAG) $(VERBOSE_FLAG) $(OPEN_BLAS_FLAG)

